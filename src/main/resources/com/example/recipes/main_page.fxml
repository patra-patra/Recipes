<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="601.0" prefWidth="605.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.recipes.Controller">
   <children>
      <ListView fx:id="myList" editable="true" layoutX="19.0" layoutY="213.0" prefHeight="282.0" prefWidth="569.0" />
      <Button fx:id="ToCart" layoutX="527.0" layoutY="179.0" mnemonicParsing="false" onAction="#SwitchToCart" text="тележка" />
      <Button fx:id="NewOne" layoutX="495.0" layoutY="179.0" mnemonicParsing="false" onAction="#SwitchToNew" text="+" />
      <Button fx:id="ParsButton" layoutX="447.0" layoutY="179.0" mnemonicParsing="false" onAction="#SwitchToPars" text="pars" />
      <Label fx:id="name" layoutX="190.0" layoutY="31.0" prefHeight="18.0" prefWidth="198.0" />
      <Button fx:id="SortName" layoutX="19.0" layoutY="506.0" mnemonicParsing="false" onAction="#SortByAlph" prefHeight="26.0" prefWidth="207.0" text="Сортировать по алфавиту" />
      <Button fx:id="Date" layoutX="19.0" layoutY="539.0" mnemonicParsing="false" onAction="#initialize" prefHeight="26.0" prefWidth="207.0" text="Сортировать по дате создания" />
      <ImageView fx:id="MainPageIMG" fitHeight="110.0" fitWidth="396.0" layoutX="111.0" layoutY="22.0" pickOnBounds="true" smooth="false">
         <image>
            <Image url="@../../../kitty.jpg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
